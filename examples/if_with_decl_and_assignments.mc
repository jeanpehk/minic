// Example of an if else statement
// Should print 2 and 3
int main() {
  // Should choose the false branch
  int a;
  a = 3;
  if (a < 3)
    a = 3; 
  else
    a = 2;
  print(a);

  // Should choose the true branch

  int b;
  b = 2;
  if (b < 3) {
    b = 3;
  }
  else {
    b = 2;
  }
  print(b);

}
